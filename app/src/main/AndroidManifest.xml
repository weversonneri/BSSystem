<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp">
        <activity android:name=".activity.ListServiceActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".activity.ListCustomerActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".activity.AddServiceActivity" />
        <activity android:name=".activity.AddCustomerActivity" />
        <activity android:name=".activity.AddScheduleActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity"/>
    </application>

</manifest>